@model Professeur
@{
    ViewBag.Title = " Contact Professeur";
    Layout = "~/Views/Shared/ProfesseurLayout.cshtml";
}
@using Personnel;
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-4">

        </div>
        <div class="col-lg-4">
            <div class="text-center card-box" style="margin-top:50px ;box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);">
                <div class="member-card">
                    <div class="thumb-lg member-thumb mx-auto"><img src="~/Photos/@Model.photo" class="rounded-circle img-thumbnail" alt="profile-image"></div>
                    <div class="">
                        <h4> @Model.prenom @Model.nom</h4>
                        <p class="text-muted"><span><a href="#" class="text-pink">Téléphone : @Model.telephone</a></span></p>
                        <p class="text-muted"><span><a href="#" class="text-pink">Email : @Model.email</a></span></p>
                        <p class="text-muted"><span><a href="#" class="text-pink">Adresse : @Model.adresse</a></span></p>

                    </div>
                    <a href="@Url.Action("ListProfesseur","Professeur")" class="btn btn-primary" style="float: right; background-color: black;"> Retour</a>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
        </div>
    </div>
</div>